{
  "name": "qrcode.vue",
  "version": "1.4.0",
  "description": "A Vue component for QRCode.",
  "main": "dist/qrcode.vue.js",
  "scripts": {
    "start": "better-npm-run start",
    "clean": "rimraf -- dist",
    "compile": "better-npm-run compile",
    "github:page": "better-npm-run github:page",
    "deploy": "better-npm-run deploy",
    "deploy:min": "better-npm-run deploy:min",
    "build": "npm run clean && npm run deploy && npm run deploy:min",
    "lint": "better-npm-run lint",
    "rollup": "rollup -c ./config/rollup.config.js"
  },
  "betterScripts": {
    "start": {
      "command": "webpack-dev-server --config webpack.config.js --open --progress --profile --watch --content-base src/",
      "env": {
        "EXAMPLE": "example",
        "DEBUG": "app:*"
      }
    },
    "compile": {
      "command": "npm run lint && node ./bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "github:page": {
      "command": "rimraf -- github-page && npm run compile",
      "env": {
        "EXAMPLE": "example",
        "MINIMIZE": "minimize",
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "deploy:min": {
      "command": "npm run compile",
      "env": {
        "MINIMIZE": "minimize",
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "lint": {
      "command": "eslint --ext .js,.vue src",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scopewu/qrcode.vue.git"
  },
  "keywords": [
    "vue",
    "vue-component",
    "qrcode"
  ],
  "author": "scope.wu <scope.wu@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scopewu/qrcode.vue/issues"
  },
  "homepage": "https://scopewu.github.io/qrcode.vue/",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "better-npm-run": "0.0.15",
    "debug": "^2.6.8",
    "eslint": "^4.2.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "html-webpack-plugin": "^2.29.0",
    "qr.js": "0.0.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.45.2",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "vue": "^2.4.1",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1"
  }
}
